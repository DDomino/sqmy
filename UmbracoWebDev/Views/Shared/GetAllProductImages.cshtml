@using UmbracoWebDev
@using WebControllers.Models

@model IEnumerable<ProdImage>

@foreach (ProdImage item in Model)
{
    <div style="border: 1px solid black; margin-bottom: 10px; padding: 5px;">
        <p><b>ID:</b> @item.Product_Id</p>
        <img src="@item.HighPic_Image" />
        <img src="@item.LowPic_Image" />
        <img src="@item.ThumbPic_Image" />
    </div>
}