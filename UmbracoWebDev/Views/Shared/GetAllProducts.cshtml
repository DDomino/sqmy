@using UmbracoWebDev

@model IEnumerable<UmbracoWebDev.Controllers.ProductViewModel>

@foreach (UmbracoWebDev.Controllers.ProductViewModel item in Model)
{
    <div style="border: 1px solid black; margin-bottom: 10px; padding: 5px;">
        <span></span><img src="@item.Product_LowPic" />
        <p>@item.Product_Title</p>
        <p>@item.Product_Name</p>
        <p>@item.Product_Description</p>

    </div>
}