@using UmbracoWebDev

@model IEnumerable<UmbracoWebDev.Controllers.PersonListViewModel>

@foreach (UmbracoWebDev.Controllers.PersonListViewModel item in Model)
{
    <div style="border: 1px solid black; margin-bottom: 10px; padding: 5px;">
        <p>@item.Id</p>
        <p>@item.Name</p>
        <p>@item.Town</p>
        <p>@item.Country</p>
    </div>
}